{
    "name": "nfephp-org/sped-console",
    "type": "console",
    "description": "Is a Console to execute commands from command line.",
    "keywords": [
        "nfe",
        "nfce",
        "sped",
        "nfephp",
        "xsd"
    ],
    "homepage": "https://github.com/nfephp-org/sped-classbuilder",
    "license": [
        "GPL-3.0+",
        "LGPL-3.0+"
    ],
    "authors": [
        {
            "name": "Antonio Spinelli",
            "email": "tonicospinelli85@gmail.com",
            "homepage": "http://www.nfephp.org",
            "role": "Developer"
        },
        {
            "name": "Comunidade NFePHP",
            "homepage": "https://github.com/nfephp-org/sped-console/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-xmlreader": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*",
        "goetas/xsd2php":"2.*@dev",
        "goetas/xsd-reader":"2.*@dev",
        "jms/serializer": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "NFePHP\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHPTest\\Console\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    },
    "bin": ["bin/sped"],
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist --testsuite all",
        "phpmd": "phpmd src text cleancode,codesize,controversial,design,naming,unusedcode",
        "phpcs": "phpcs --standard=PSR2 src"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/goetas/serializer.git"
        }
    ]
}
